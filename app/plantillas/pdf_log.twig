<!DOCTYPE html>
<html>
<head>
    <title>Ventas</title>
    <style>
        table {
            border-collapse: collapse;
        }
        th, td {
            border: 1px solid black;
            padding: 5px;
        }
    </style>
</head>
<body>
    <table>
        <thead>
            <tr>
               <th>FILA</th>
                <th>ID</th>
                <th>ID_USUARIO</th>
                <th>ID_ARMA</th>
                <th>ACCION</th>
                <th>FECHA</th>
            </tr>
        </thead>
        <tbody>
            {% for venta, datos in ventas %}
                <tr>
                    <td>{{ venta }}</td>
                    {% set keys = datos|keys %}{% for key in keys %}
                    <td>{{ datos[key] }}</td>
                    {% endfor %}
                </tr>
            {% endfor %}
        </tbody>
    </table>
</body>
</html>